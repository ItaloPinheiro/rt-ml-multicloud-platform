{
  "uid": "feature-store",
  "title": "2. Feature Store",
  "panels": [
    {
      "type": "stat",
      "title": "Cache Hit Rate",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 6
      },
      "targets": [
        {
          "expr": "(rate(redis_keyspace_hits_total[5m]) / (rate(redis_keyspace_hits_total[5m]) + rate(redis_keyspace_misses_total[5m]))) * 100",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "orange",
                "value": 50
              },
              {
                "color": "green",
                "value": 80
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Cache Hits vs Misses",
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 18,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum(rate(redis_keyspace_hits_total[5m]))",
          "legendFormat": "Hits",
          "refId": "A"
        },
        {
          "expr": "sum(rate(redis_keyspace_misses_total[5m]))",
          "legendFormat": "Misses",
          "refId": "B"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "fillOpacity": 10
          }
        },
        "overrides": []
      }
    }
  ],
  "schemaVersion": 36,
  "timezone": "browser",
  "refresh": "1m"
}